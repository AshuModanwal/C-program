#include <stdio.h>
#include <math.h>
int main()
{
    int year;
    printf("Enter the year : ");
    scanf("%d", &year);
    if (year % 400 == 0)
    {
        printf("The year %d is leap year.", year);
    }
    else if (year % 100 != 0 && year % 4 == 0)
    {
        printf("The year %d is leap year.", year);
    }

    else
    {
        printf("the year %d is not leapyear.", year);
    }
    return 0;
}
